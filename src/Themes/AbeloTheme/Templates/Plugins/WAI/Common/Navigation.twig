<!-- OffCanvas Search Start -->
<div id="offcanvas-mobile-menu" class="offcanvas offcanvas-mobile-menu">
  <div class="inner customScroll">
    <div class="head">
      <span class="title">&nbsp;</span>
      <button class="offcanvas-close">×</button>
    </div>
    <div class="offcanvas-menu-search-form">
      <form action="#">
        <input type="text" placeholder="{{ translate("Search...", "Common") }}">
        <button><i class="lnr lnr-magnifier"></i></button>
      </form>
    </div>
    <div class="offcanvas-menu">
      <ul>
        {% if navigationItems is iterable %}
          {% for item in navigationItems %}
            <li><a href="{{ item.url }}"><span class="menu-text">{{ item.text }}</span></a>
              {% if item.children is iterable %}
                <ul class="sub-menu">
                  {% for itemSub in item.children %}
                    <li><a href="{{ itemSub.url }}"><span class="menu-text">{{ itemSub.text }}</span></a></li>
                  {% endfor %}
                </ul>
              {% endif %}
            </li>
          {% endfor %}
        {% endif %}
      </ul>
    </div>
    <!-- OffCanvas Menu End -->
    <div class="offcanvas-social mt-30px">
      <ul>
        {% if settings.profile.urlFacebook != "" %}
          <li><a href="{{ settings.profile.urlFacebook }}"><i class="ion-social-facebook"></i></a></li>
        {% endif %}
        {% if settings.profile.urlTwitter != "" %}
          <li><a href="{{ settings.profile.urlTwitter }}"><i class="ion-social-twitter"></i></a></li>
        {% endif %}
        {% if settings.profile.urlYouTube != "" %}
          <li><a href="{{ settings.profile.urlYouTube }}"><i class="ion-social-youtube"></i></a></li>
        {% endif %}
        {% if settings.profile.urlInstagram != "" %}
          <li><a href="{{ settings.profile.urlInstagram }}"><i class="ion-social-instagram"></i></a></li>
        {% endif %}
      </ul>
    </div>
  </div>
</div>
<!-- OffCanvas Search End -->