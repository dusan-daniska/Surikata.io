  <div class="header-nav">
    <div class="container">
      <div class="header-nav-wrapper d-md-flex d-sm-flex d-xl-flex d-lg-flex justify-content-between">

        <div>
          {% if slogan|length > 0 %}
            <div class="header-nav-slogan">
              {{ slogan|raw }}
            </div>
          {% endif %}
          {% if shortContact|length > 0 %}
            <div class="header-short-contact" style="{{ shortContact|length > 0 ? "border-left: 1px solid #c6c6c6;" : "" }}">
              {{ shortContact|raw }}
            </div>
          {% endif %}
        </div>

        <div class="header-static-nav">
          {% if user.isLogged %}
            <a href="{{ rootUrl }}/{{ callPluginMethod('WAI/Customer/Home', 'getWebPageUrl') }}">{{ user.profile.email }}</a>
           {% endif %}
        </div>
        <div class="header-menu-nav">
          <ul class="menu-nav">
            <li>
              {% if user.isLogged %}
                <div class="dropdown">
                  <button type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">{{ translate("My account", "Common") }} <i class="ion-ios-arrow-down"></i></button>
                  <ul class="dropdown-menu animation slideDownIn" aria-labelledby="dropdownMenuButton">
                    <li><a href="{{ rootUrl }}/{{ callPluginMethod('WAI/Customer/Home', 'getWebPageUrl') }}">{{ translate("My account", "Common") }}</a></li>
                    <li><a href="{{ rootUrl }}/{{ callPluginMethod('WAI/Customer/Home', 'getWebPageUrl') }}">{{ translate("Checkout", "Common") }}</a></li>
                    <li><a href='?doLogout=1'>{{ translate("Logout", "Common") }}</a></li>
                  </ul>
                </div>
              {% else %}
                <a href="{{ rootUrl }}/{{ callPluginMethod('WAI/Customer/Login', 'getWebPageUrl') }}">{{ translate("Login") }}</a>
              {% endif %}
