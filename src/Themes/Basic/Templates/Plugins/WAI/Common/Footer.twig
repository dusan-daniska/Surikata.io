            <li><a href="{{ rootUrl }}/about-us">About</a></li>
            <li><a href="{{ rootUrl }}/products">Products</a></li>
            <li><a href="{{ rootUrl }}/blog">Blog</a></li>
            <li><a href="{{ rootUrl }}/contact">Contact</a></li>
          </ul>
        </div>

        <div class="col-md-5 sign-gd-two">
          <h4>Store <span>Information</span></h4>
          <div class="address">
            <div class="address-grid">
              <div class="address-left">
                <i class="fa fa-phone" aria-hidden="true"></i>
              </div>
              <div class="address-right">
                {% if showContactPhoneNumber %}
                  <h6>Phone Number</h6>
                  <p><a href="">{{ settings.companyInfo.contactPhoneNumber }}</a></p>
                {% endif %}
              </div>
              <div class="clearfix"> </div>
            </div>
            <div class="address-grid">
              <div class="address-left">
                <i class="fa fa-envelope" aria-hidden="true"></i>
              </div>
              <div class="address-right">
                {% if showContactEmail %}
                  <h6>Email Address</h6>
                  <p><a href="mailto:{{ settings.companyInfo.contactEmail }}">{{ settings.companyInfo.contactEmail }}</a></p>
                {% endif %}
              </div>
              <div class="clearfix"> </div>
            </div>
            <div class="address-grid">
              <div class="address-left">
                <i class="fa fa-map-marker" aria-hidden="true"></i>
              </div>
              <div class="address-right">
                {% if showContactAddress %}
                  <h6>Location</h6>
                  <p>
                    {{ settings.companyInfo.sidlo_ulica_1 }} {{ settings.companyInfo.sidlo_ulica_2 }}
                    {{ settings.companyInfo.sidlo_psc }} {{ settings.companyInfo.sidlo_mesto }}
                  </p>
                {% endif %}
              </div>
              <div class="clearfix"> </div>
            </div>
          </div>
        </div>
        <div class="col-md-3 sign-gd flickr-post">
          <h4>Links <span></span></h4>
          <ul>
            {% if showMainMenu %}
              {% for menuItem in mainMenuItems %}
                <li><a href="{{ rootUrl }}/{{ menuItem.url }}">{{ menuItem.text }}</a></li>
              {% endfor %}
            {% endif %}
          </ul>
        </div>
        <div class="clearfix"></div>
      </div>
    </div>
    <div class="clearfix"></div>

