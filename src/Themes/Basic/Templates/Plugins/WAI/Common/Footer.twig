        {% if settings.companyInfo.urlTwitter != "" %}
          <li>
            <a href="{{ settings.companyInfo.urlTwitter }}" class="twitter">
              <div class="front" style="background: var(--theme-second-color);"><i class="fa fa-twitter" aria-hidden="true"></i></div>
              <div class="back"><i class="fa fa-twitter" aria-hidden="true"></i></div>
            </a>
          </li>
        {% endif %}
        {% if settings.companyInfo.urlYouTube != "" %}
          <li>
            <a href="{{ settings.companyInfo.urlYouTube }}" class="pinterest">
